 'Fission Neutron Spectrum' 
 300.0 
 1.0e10 
  3 261 'spect' /
 0 /
 0 /
errorr
 20 0 91 28 0 0 /
 9861 19 6 1 1 /
 1 300.0 /
 1 33 /
 1 /
  261  /
 1 /
 0.0001  20000000. /
covr
 28 0 38 /
 1 /
 1.e3 /
 /
 9861 /
viewr
 38 39 /
stop
'@'	echo "This starts the post-process command stream"
'@'	echo " "
'@'	rm tape20
'@'     cp output             ../output/Cf252_640nrg_IRDFF-II_MF33.out
'@'	cp tape39             ../correlation/Cf252_640nrg_IRDFF-II_MF33.eps
