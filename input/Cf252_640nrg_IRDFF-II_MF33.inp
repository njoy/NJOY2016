'@'	cp lsl_interface.cov  ../correlation/Cf252_640nrg_IRDFF-II_MF33.lsl
'@'	cp tape91             ../groupr/Cf252_640nrg_IRDFF-II_MF33.groupr
'@'	echo " "
'@'	echo "This ends the post-process command stream"
